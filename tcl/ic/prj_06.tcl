    # построения сетки в этой области.    
    del_asm {DM DG2 DG3 DG4 DG5 DG6 DG7}
    del_asm {C/M C/G2 C/G6 C/G7}
}

# Здесь нужно обновить топологию

proc msh_dg1 {} {
    ch_all;   # Перенос кривых в семейства ицидентных с ними поверхностей
